/* parsable */
let 
    type arr = array of int
    var arr1 := arr [5] of 0

    function midpoint(arr1:arr, low:int, high:int) : int =
        if low < high then
            let var l := low + 1
                var h := high - 1
            in midpoint(arr1, l, h) end
        else arr1[low]
    
in
    arr1[0] := 1;
    arr1[1] := 2;
    arr1[2] := 3;
    arr1[3] := 4;
    arr1[4] := 5;
    midpoint(arr1,0,4)

end
