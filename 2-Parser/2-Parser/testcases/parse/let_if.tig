   operators, unary negation, and function declaration and call
*/
let
	var a := 1
	var string1 := "abcde"
	var string2 := "abced"
	function dumbstreq(s1 : string, s2:string) : int = 
		(s1 < s2 | s1 > s2) <> 1
in
	(
		let
			var ONE_THOUSAND := 1000
		in
		end;
		if 1 <> 2 then
			()
		else
			(1+1; ());
		if (string1=string2)=dumbstreq(string1, string2) then
			-5 + 3
		else
			-(5+3)
	)
end
